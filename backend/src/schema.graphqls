}

# Paging on the cursor will return commits from all branches, you can not filter out commits from a single branch this way.
# If you want get a paged list of commits for a single branch you can page off of the parentCommit instead of the cursor.
type PageableCommit {
  items: [Commit!]!
  cursor: Timestamp
  parentCommit: String
}

type PageableJob {
  items: [Job!]!
  cursor: Timestamp
  hasNextPage: Boolean
}

type PageableJobSet {
  items: [JobSet!]!
  cursor: Timestamp
  hasNextPage: Boolean
}

type PageableFile {
  cursor: String
  hasNextPage: Boolean
  files: [File!]!
}

