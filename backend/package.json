{
  "name": "@pachyderm/dash-backend",
  "version": "0.0.1",
  "private": true,
  "proxy": "http://localhost:3000",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "generate-types": "graphql-codegen",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "NODE_PATH=dist/ NODE_ENV=production DOTENV_FLOW_PATH=../ node -r module-alias/register -r dotenv-flow/config dist/index.js",
    "start:dev": "LOG_LEVEL=debug NODE_ENV=development DOTENV_FLOW_PATH=../ nodemon --watch ./src --ext ts -- --files -r dotenv-flow/config -r tsconfig-paths/register src/index.ts",
    "start:test": "NODE_ENV=test DOTENV_FLOW_PATH=../ nodemon --watch ./src --ext ts -- --files -r dotenv-flow/config -r tsconfig-paths/register src/index.ts",
    "start:mock": "NODE_PATH=dist/ NODE_ENV=test DOTENV_FLOW_PATH=../ MOCK_AUTH_PORT=30658 MOCK_GRPC_PORT=50051 nodemon --watch ./src --ext ts -- --files -r dotenv-flow/config -r tsconfig-paths/register src/mock/index.ts",
    "mock-and-start": "npm run start:test & npm run start:mock",
    "test": "NODE_ENV=test GRAPHQL_PORT=0 LOG_LEVEL=none dotenv-flow -p ../ -- jest --max-workers=6"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.2.2",
    "@pachyderm/node-pachyderm": "0.13.1",
    "@sentry/node": "6.8.0",
    "@sentry/tracing": "6.8.0",
    "apollo-server-express": "2.19.0",
    "bunyan": "1.8.15",
    "cookie-parser": "1.4.5",
    "dotenv-flow": "3.2.0",
    "dotenv-flow-cli": "1.0.0",
    "ejs": "3.1.6",
    "express": "4.17.1",
    "filesize": "6.2.6",
    "graphql": "15.5.0",
    "graphql-ws": "5.3.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "mime-types": "2.1.29",
    "module-alias": "^2.2.2",
    "object-hash": "2.1.1",
    "openid-client": "4.4.0",
    "tar-stream": "2.2.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@apollo/client": "3.3.1",
    "@pachyderm/config": "0.6.0",
    "@types/bunyan": "1.8.6",
    "@types/cookie-parser": "1.4.2",
    "@types/ejs": "3.0.6",
    "@types/jest": "26.0.15",
    "@types/jsonwebtoken": "8.5.0",
    "@types/lodash": "4.14.167",
    "@types/mime-types": "2.1.0",
    "@types/object-hash": "2.1.0",
    "@types/tar-stream": "^2.2.0",
    "@types/uuid": "8.3.0",
    "@types/ws": "7.4.1",
    "cors": "2.8.5",
    "cross-fetch": "3.1.4",
    "eslint": "6.8.0",
    "jest": "26.6.3",
    "node-jq": "1.12.0",
    "nodemon": "2.0.7",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.1.2",
    "ws": "7.4.6"
  },
  "_moduleAliases": {
    "@dash-backend": "./dist",
    "@graphqlTypes": "./dist/generated/types.js"
  }
}
