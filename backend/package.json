{
  "name": "@pachyderm/dash-backend",
  "version": "0.0.1",
  "private": true,
  "proxy": "http://localhost:3000",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json && cp -r ./src/proto/proto ./dist/proto/proto",
    "generate-types": "graphql-codegen",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "NODE_PATH=dist/ NODE_ENV=production DOTENV_FLOW_PATH=../ node -r module-alias/register -r dotenv-flow/config dist/index.js",
    "start:dev": "LOG_LEVEL=debug NODE_ENV=development DOTENV_FLOW_PATH=../ nodemon --watch ./src --ext ts -- --files -r dotenv-flow/config -r tsconfig-paths/register src/index.ts",
    "start:test": "NODE_ENV=test DOTENV_FLOW_PATH=../ nodemon --watch ./src --ext ts -- --files -r dotenv-flow/config -r tsconfig-paths/register src/index.ts",
    "start:mock": "NODE_PATH=dist/ NODE_ENV=test DOTENV_FLOW_PATH=../ MOCK_AUTH_PORT=30658 MOCK_GRPC_PORT=30650 nodemon --watch ./src --ext ts -- --files -r dotenv-flow/config -r tsconfig-paths/register src/mock/index.ts",
    "mock-and-start": "npm run start:test & npm run start:mock",
    "test": "NODE_ENV=test GRAPHQL_PORT=0 LOG_LEVEL=none dotenv-flow -p ../ -- jest \"^(.*/)*(.*\\.)*(test)\\.ts$\" --max-workers=6",
    "integration-test": "NODE_ENV=test dotenv-flow -p ../ -- jest \"^(.*/)*(.*\\.)*(integration)\\.ts$\" --max-workers=1"
  },
  "dependencies": {
    "@graphql-tools/schema": "9.0.4",
    "@grpc/grpc-js": "1.7.1",
    "@rudderstack/rudder-sdk-node": "1.1.4",
    "@sentry/node": "7.13.0",
    "@sentry/tracing": "7.22.0",
    "@types/google-protobuf": "^3.15.6",
    "@types/node": "^18.11.10",
    "apollo-server-errors": "3.3.1",
    "apollo-server-express": "3.10.2",
    "bunyan": "1.8.15",
    "busboy": "1.5.0",
    "cookie-parser": "1.4.6",
    "dotenv-flow": "3.2.0",
    "dotenv-flow-cli": "1.0.0",
    "ejs": "3.1.8",
    "express": "4.18.1",
    "filesize": "9.0.11",
    "google-protobuf": "^3.21.0",
    "graphql": "16.6.0",
    "graphql-subscriptions": "2.0.0",
    "graphql-ws": "5.6.3",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "mime-types": "2.1.35",
    "module-alias": "2.2.2",
    "object-hash": "3.0.0",
    "openid-client": "4.4.0",
    "p-queue": "6.6.2",
    "retry": "^0.13.1",
    "uuid": "9.0.0"
  },
  "devDependencies": {
    "@apollo/client": "3.5.10",
    "@swc/jest": "^0.2.22",
    "@types/bunyan": "1.8.8",
    "@types/busboy": "1.5.0",
    "@types/cookie-parser": "1.4.3",
    "@types/ejs": "3.1.1",
    "@types/jest": "27.4.1",
    "@types/jsonwebtoken": "8.5.9",
    "@types/lodash": "4.14.185",
    "@types/mime-types": "2.1.1",
    "@types/object-hash": "2.2.1",
    "@types/retry": "^0.12.2",
    "@types/tar-stream": "2.2.2",
    "@types/uuid": "8.3.4",
    "@types/ws": "8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "apollo-server-plugin-base": "3.6.2",
    "cors": "2.8.5",
    "cross-fetch": "3.1.5",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^27.1.6",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-testing-library": "5.9.1",
    "jest": "27.5.1",
    "node-jq": "2.3.3",
    "nodemon": "2.0.20",
    "ts-jest": "^27.1.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.1",
    "typescript": "4.9.3",
    "ws": "8.8.1"
  },
  "_moduleAliases": {
    "@dash-backend": "./dist",
    "@graphqlTypes": "./dist/generated/types.js"
  }
}
