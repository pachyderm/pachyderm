{
  "name": "@pachyderm/dash-backend",
  "version": "0.0.1",
  "private": true,
  "proxy": "http://localhost:3000",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "generate-types": "graphql-codegen",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "NODE_PATH=dist/ NODE_ENV=production DOTENV_FLOW_PATH=../ node -r module-alias/register -r dotenv-flow/config dist/index.js",
    "start:dev": "LOG_LEVEL=debug NODE_ENV=development DOTENV_FLOW_PATH=../ nodemon --watch ./src --ext ts -- --files -r dotenv-flow/config -r tsconfig-paths/register src/index.ts",
    "start:test": "NODE_ENV=test DOTENV_FLOW_PATH=../ nodemon --watch ./src --ext ts -- --files -r dotenv-flow/config -r tsconfig-paths/register src/index.ts",
    "start:mock": "NODE_PATH=dist/ NODE_ENV=test DOTENV_FLOW_PATH=../ MOCK_AUTH_PORT=30658 MOCK_GRPC_PORT=30650 nodemon --watch ./src --ext ts -- --files -r dotenv-flow/config -r tsconfig-paths/register src/mock/index.ts",
    "mock-and-start": "npm run start:test & npm run start:mock",
    "test": "NODE_ENV=test GRAPHQL_PORT=0 LOG_LEVEL=none dotenv-flow -p ../ -- jest --max-workers=6"
  },
  "dependencies": {
    "@graphql-tools/schema": "8.3.5",
    "@grpc/grpc-js": "1.5.9",
    "@pachyderm/node-pachyderm": "0.28.0",
    "@rudderstack/rudder-sdk-node": "1.1.3",
    "@sentry/node": "6.19.1",
    "@sentry/tracing": "6.19.1",
    "apollo-server-errors": "3.3.1",
    "apollo-server-express": "3.6.6",
    "bunyan": "1.8.15",
    "busboy": "1.5.0",
    "cookie-parser": "1.4.6",
    "dotenv-flow": "3.2.0",
    "dotenv-flow-cli": "1.0.0",
    "ejs": "3.1.6",
    "express": "4.17.1",
    "filesize": "8.0.7",
    "graphql": "16.3.0",
    "graphql-subscriptions": "2.0.0",
    "graphql-ws": "5.6.3",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "mime-types": "2.1.35",
    "module-alias": "2.2.2",
    "object-hash": "3.0.0",
    "openid-client": "4.4.0",
    "p-queue": "6.6.2",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@apollo/client": "3.5.10",
    "@pachyderm/config": "0.8.0",
    "@types/bunyan": "1.8.8",
    "@types/busboy": "1.5.0",
    "@types/cookie-parser": "1.4.2",
    "@types/ejs": "3.1.0",
    "@types/jest": "27.4.1",
    "@types/jsonwebtoken": "8.5.8",
    "@types/lodash": "4.14.157",
    "@types/mime-types": "2.1.1",
    "@types/object-hash": "2.2.1",
    "@types/tar-stream": "2.2.2",
    "@types/uuid": "8.3.4",
    "@types/ws": "8.5.3",
    "apollo-server-plugin-base": "3.5.2",
    "cors": "2.8.5",
    "cross-fetch": "3.1.5",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-react-app": "^7.0.0",
    "eslint-import-resolver-typescript": "^2.7.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-testing-library": "^5.1.0",
    "jest": "27.5.1",
    "node-jq": "1.12.0",
    "nodemon": "2.0.15",
    "ts-jest": "^27.1.3",
    "ts-node": "10.7.0",
    "tsconfig-paths": "3.14.1",
    "typescript": "4.6.2",
    "ws": "8.5.0"
  },
  "_moduleAliases": {
    "@dash-backend": "./dist",
    "@graphqlTypes": "./dist/generated/types.js"
  }
}
