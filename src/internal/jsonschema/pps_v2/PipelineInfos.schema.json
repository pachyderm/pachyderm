            "description": "//  PFS Data structures (stored in etcd)"
        },
        "pfs_v2.SQLDatabaseEgress": {
            "properties": {
                "url": {
                    "type": "string"
                },
                "fileFormat": {
                    "$ref": "#/definitions/pfs_v2.SQLDatabaseEgress.FileFormat",
