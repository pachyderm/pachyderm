                            "REPO_INSPECT_COMMIT",
                            "REPO_LIST_COMMIT",
                            "REPO_DELETE_COMMIT",
                            "REPO_CREATE_BRANCH",
                            "REPO_LIST_BRANCH",
                            "REPO_DELETE_BRANCH",
                            "REPO_INSPECT_FILE",
                            "REPO_LIST_FILE",
                            "REPO_ADD_PIPELINE_READER",
