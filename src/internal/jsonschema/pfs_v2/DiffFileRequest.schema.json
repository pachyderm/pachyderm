                "old_file": {
                    "$ref": "#/definitions/pfs_v2.File",
                    "additionalProperties": false,
                    "description": "OldFile may be left nil in which case the same path in the parent of NewFile's commit will be used."
                },
                "oldFile": {
                    "$ref": "#/definitions/pfs_v2.File",
                    "additionalProperties": false,
                    "description": "OldFile may be left nil in which case the same path in the parent of NewFile's commit will be used."
                },
                "shallow": {
                    "type": "boolean"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Diff File Request"
        },
        "pfs_v2.Branch": {
