{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/ListRepoRequest",
    "definitions": {
        "ListRepoRequest": {
            "properties": {
                "type": {
                    "type": "string",
                    "description": "type is the type of (system) repos that should be returned an empty string requests all repos"
                },
                "projects": {
                    "items": {
                        "$ref": "#/definitions/pfs_v2.Project"
                    },
                    "additionalProperties": false,
                    "type": "array",
                    "description": "projects filters out repos that do not belong in the list, while no projects means list all repos."
                },
                "page": {
                    "$ref": "#/definitions/pfs_v2.RepoPage",
                    "additionalProperties": false,
                    "description": "repo page defines the"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "List Repo Request"
        },
        "pfs_v2.Project": {
            "properties": {
                "name": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Project"
        },
        "pfs_v2.RepoPage": {
            "properties": {
                "order": {
                    "enum": [
                        "PROJECT_REPO"
                    ],
                    "type": "string",
                    "title": "Ordering"
                },
                "pageSize": {
                    "type": "integer"
                },
                "pageIndex": {
                    "type": "integer"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Repo Page"
        }
    }
}