{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/Starlark",
    "definitions": {
        "Starlark": {
            "properties": {
                "builtin": {
                    "type": "string",
                    "description": "One built into the pachd binary."
                },
                "script": {
                    "$ref": "#/definitions/debug_v2.StarlarkScript",
                    "additionalProperties": false,
                    "description": "Or a script supplied in this request."
                },
                "timeout": {
                    "pattern": "^([0-9]+\\.?[0-9]*|\\.[0-9]+)s$",
                    "type": "string",
                    "description": "How long to allow the script to run for.  If unset, defaults to 1 minute.",
                    "format": "regex"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "oneOf": [
                {
                    "required": [
                        "builtin"
                    ]
                },
                {
                    "required": [
                        "script"
                    ]
                }
            ],
            "title": "Starlark",
            "description": "Starlark controls the running of a Starlark script."
        },
        "debug_v2.StarlarkScript": {
            "properties": {
                "name": {
                    "type": "string",
                    "description": "The name of the script; used for debug messages and to control where the output goes."
                },
                "programText": {
                    "type": "string",
                    "description": "The text of the \"debugdump\" peronality Starlark program."
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Starlark Script",
            "description": "StarlarkScript is a Starlark script."
        }
    }
}