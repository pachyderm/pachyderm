{
  "name": "@pachyderm/dash-frontend",
  "version": "0.0.1",
  "private": true,
  "proxy": "http://localhost:3000",
  "scripts": {
    "analyze": "ANALYZE_WEBPACK_BUILD=true npm run build",
    "build": "NODE_ENV=production dotenv-flow -p ../  -- webpack",
    "build:staging": "NODE_ENV=staging dotenv-flow -p ../ -- webpack",
    "cypress:headless": "NODE_ENV=test dotenv-flow -p ../ -- cypress run --browser chrome --headless --config video=false",
    "cypress:open": "NODE_ENV=test dotenv-flow -p ../ -- cypress open",
    "e2e:open": "start-server-and-test start http://localhost:4000 cypress:headless",
    "e2e": "start-server-and-test start http://localhost:4000 cypress:headless",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "npx eslint '{src,cypress}/**/*.{ts,tsx}'",
    "lint:css": "npx stylelint 'src/**/*.module.css'",
    "serve": "PORT=4000 serve -s build",
    "start:mock": "NODE_ENV=test PORT=4000 dotenv-flow -p ../ -- node ./node_modules/@pachyderm/scripts/startDev.js",
    "start": "NODE_ENV=development PORT=4000 dotenv-flow -p ../ -- node ./node_modules/@pachyderm/scripts/startDev.js",
    "test": "NODE_ENV=test GRAPHQL_PORT=0 LOG_LEVEL=none dotenv-flow -p ../ -- jest --max-workers=6",
    "tslint": "npx tslint 'src/**/*.{ts,tsx}'",
    "generate-hooks": "graphql-codegen"
  },
  "dependencies": {
    "@apollo/client": "3.3.1",
    "@pachyderm/components": "0.15.2",
    "@pachyderm/polyfills": "0.0.1",
    "@sentry/browser": "5.19.1",
    "classnames": "2.2.6",
    "d3": "6.3.1",
    "date-fns": "2.14.0",
    "graphql": "15.5.0",
    "graphql-anywhere": "4.2.7",
    "js-cookie": "2.2.1",
    "lodash": "4.17.21",
    "react": "17.0.1",
    "react-bootstrap": "1.1.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-hook-form": "6.15.4",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "subscriptions-transport-ws": "0.9.18"
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.9.5",
    "@graphql-codegen/add": "2.0.2",
    "@graphql-codegen/cli": "1.17.3",
    "@graphql-codegen/import-types-preset": "1.18.1",
    "@graphql-codegen/typescript": "1.20.0",
    "@graphql-codegen/typescript-operations": "1.17.13",
    "@graphql-codegen/typescript-react-apollo": "2.2.3",
    "@pachyderm/config": "0.5.0",
    "@pachyderm/scripts": "0.5.4",
    "@sentry/cli": "1.55.0",
    "@testing-library/cypress": "7.0.6",
    "@testing-library/dom": "7.29.4",
    "@testing-library/jest-dom": "5.11.8",
    "@testing-library/react": "11.2.3",
    "@testing-library/user-event": "12.6.0",
    "@types/chart.js": "2.9.25",
    "@types/chartjs-plugin-crosshair": "1.1.0",
    "@types/classnames": "2.2.10",
    "@types/d3": "^6.2.0",
    "@types/jest": "26.0.15",
    "@types/js-cookie": "2.2.6",
    "@types/lodash": "4.14.157",
    "@types/node": "14.0.20",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-helmet": "6.1.0",
    "@types/react-router-dom": "5.1.5",
    "canvas": "2.6.1",
    "cross-fetch": "3.0.6",
    "cypress": "7.2.0",
    "cypress-wait-until": "1.7.1",
    "dotenv-flow": "3.2.0",
    "dotenv-flow-cli": "1.0.0",
    "es6-promise": "4.2.8",
    "jest": "26.6.3",
    "jest-fetch-mock": "3.0.3",
    "serve": "11.3.2",
    "start-server-and-test": "1.11.0",
    "ts-node": "9.0.0",
    "tsconfig-paths": "3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "4.1.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
