{
  "name": "@pachyderm/dash-backend",
  "version": "0.0.1",
  "private": true,
  "proxy": "http://localhost:3000",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.json && cp src/mock/mockPrivate.key dist/mock/mockPrivate.key",
    "generate-types": "graphql-codegen",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "NODE_PATH=dist/ node -r dotenv-flow/config dist/index.js",
    "start:dev": "NODE_ENV=test ts-node --files -r dotenv-flow/config -r tsconfig-paths/register src/index.ts",
    "start:mock": "NODE_PATH=dist/ NODE_ENV=test ts-node --files -r dotenv-flow/config -r tsconfig-paths/register src/mock/index.ts",
    "mock-and-start": "npm run start:dev & npm run start:mock",
    "test": "jest"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.2.2",
    "@pachyderm/proto": "2.0.0-alpha.5.2",
    "apollo-server-express": "2.19.0",
    "express": "4.17.1",
    "lodash": "4.17.20",
    "openid-client": "4.4.0"
  },
  "devDependencies": {
    "@graphql-codegen/add": "2.0.1",
    "@graphql-codegen/cli": "1.19.2",
    "@graphql-codegen/typescript": "1.17.11",
    "@graphql-codegen/typescript-resolvers": "1.18.1",
    "@pachyderm/config": "0.5.0",
    "@types/lodash": "4.14.167",
    "dotenv-flow": "3.2.0",
    "eslint": "6.8.0",
    "graphql-request": "3.4.0",
    "jest": "26.6.3",
    "jsonwebtoken": "8.5.1",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.1.2"
  }
}
