{
  "pipeline": {
    "name": "model"
  },
  "description": "A pipeline that trains the model based on the data in the `training` repository by using a Python script.",
  "transform": {
    "image": "pachyderm/iris-train:python-svm",
    "cmd": [
	"python3",
	"/code/pytrain.py",
	"/pfs/training/",
	"/pfs/out/"
    ]
  },
  "parallelism_spec": {
    "constant": "1"
  },
  "input": {
    "pfs": {
      "repo": "training",
      "glob": "/"
    }
  }
}
