{
  "pipeline": {
    "name": "model"
  },
  "description": "A pipeline that trains the model based on the data in the `training` repository by using an R script.",
  "transform": {
    "image": "pachyderm/iris-train:rstats-svm",
    "cmd": [
	"Rscript",
	"train.R"
    ]
  },
  "parallelism_spec": {
    "constant": "1"
  },
  "input": {
    "pfs": {
      "repo": "training",
      "glob": "/"
    }
  }
}
