{
  "pipeline": {
    "name": "inference"
  },
  "description": "An inference pipeline that makes a prediction based on the trained model by using a Python script.",
  "transform": {
    "image": "pachyderm/iris-infer:python",
    "cmd": [
      "python3",
      "/code/pyinfer.py",
      "/pfs/model/",
      "/pfs/attributes/",
      "/pfs/out/"
    ]
  },
  "parallelism_spec": {
    "constant": "1"
  },
  "input": {
    "cross": [
      {
        "pfs": {
          "repo": "attributes",
          "glob": "/*"
        }
      },
      {
        "pfs": {
          "repo": "model",
          "glob": "/"
        }
      }
    ]
  }
}
