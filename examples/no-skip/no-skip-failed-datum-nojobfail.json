{
  "pipeline": {
    "name": "no-skip-test"
  },
  "enable_stats": true,
  "description": "A pipeline that tests the no-skip field.",
  "input": {
    "pfs": {
      "glob": "/*",
      "repo": "model"
    }
  },
  "no_skip": true,
  "transform": {
    "cmd": [ "bash"],
    "stdin": ["if [ -a /pfs/input/file1.txt ]; then exit 1 ; fi ; exit 0"],
    "err_cmd": [ "bash" ],
    "err_stdin": ["if [ -a /pfs/input/file3.txt ]; then exit 0 ; fi ; exit 1"]
}
}
