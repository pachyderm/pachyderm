{
  "service" : {
    "internal_port": 8888,
    "external_port": 30888
  },
  "transform": {
    "image": "pachyderm/pachyderm_jupyter",
    "cmd": [ "sh" ],
    "stdin": [
		"/opt/conda/bin/jupyter notebook"
    ]
  },
  "parallelism_spec": {
    "constant": 1
  },
  "input": {
    "cross": [
      {
        "atom": {"repo": "trips"}
      },
      {
        "atom": {"repo": "weather"}
      },
      {
        "atom": {"repo": "sales"}
      }
    ]
  }
}
