{
  "name": "@pachyderm/dash-frontend",
  "version": "0.0.1",
  "private": true,
  "proxy": "http://localhost:3000",
  "scripts": {
    "analyze": "ANALYZE_WEBPACK_BUILD=true npm run build",
    "build": "NODE_ENV=production dotenv-flow -p ../  -- webpack",
    "build:staging": "NODE_ENV=staging dotenv-flow -p ../ -- webpack",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "npx eslint '{src,cypress}/**/*.{ts,tsx}'",
    "lint:css": "npx stylelint 'src/**/*.module.css'",
    "serve": "PORT=4000 serve -s build",
    "start:mock": "NODE_ENV=test PORT=4000 dotenv-flow -p ../ -- node ./node_modules/@pachyderm/scripts/startDev.js",
    "start": "NODE_ENV=development PORT=4000 dotenv-flow -p ../ -- node ./node_modules/@pachyderm/scripts/startDev.js",
    "test": "NODE_ENV=test GRAPHQL_PORT=0 LOG_LEVEL=none dotenv-flow -p ../ -- jest --max-workers=6",
    "tslint": "npx tslint 'src/**/*.{ts,tsx}'",
    "generate-hooks": "graphql-codegen"
  },
  "dependencies": {
    "@apollo/client": "3.3.1",
    "@pachyderm/components": "1.17.13",
    "@pachyderm/polyfills": "0.1.0",
    "@sentry/browser": "5.19.1",
    "@sentry/react": "6.8.0",
    "@sentry/tracing": "6.8.0",
    "classnames": "2.2.6",
    "d3": "6.3.1",
    "date-fns": "2.14.0",
    "elkjs": "0.7.1",
    "graphql": "15.5.0",
    "graphql-ws": "5.3.0",
    "js-cookie": "2.2.1",
    "lodash": "4.17.21",
    "object-hash": "2.1.1",
    "react": "17.0.1",
    "react-bootstrap": "1.1.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "react-hook-form": "7.5.2",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-virtualized-auto-sizer": "1.0.5",
    "react-window": "1.8.6",
    "rudder-sdk-js": "1.0.16"
  },
  "devDependencies": {
    "@graphql-tools/graphql-file-loader": "^7.3.3",
    "@graphql-tools/load": "^7.4.1",
    "@pachyderm/config": "0.7.0",
    "@pachyderm/scripts": "0.7.0",
    "@sentry/cli": "1.55.0",
    "@testing-library/dom": "7.29.4",
    "@testing-library/jest-dom": "5.11.8",
    "@testing-library/react": "11.2.3",
    "@testing-library/user-event": "12.6.0",
    "@types/chart.js": "2.9.25",
    "@types/chartjs-plugin-crosshair": "1.1.0",
    "@types/classnames": "2.2.10",
    "@types/d3": "^6.2.0",
    "@types/jest": "26.0.15",
    "@types/js-cookie": "2.2.6",
    "@types/lodash": "4.14.157",
    "@types/node": "14.0.20",
    "@types/object-hash": "2.1.1",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-helmet": "6.1.0",
    "@types/react-router-dom": "5.1.5",
    "@types/react-virtualized-auto-sizer": "1.0.0",
    "@types/react-window": "1.8.3",
    "@types/sanitize-html": "^2.3.1",
    "canvas": "2.8.0",
    "cross-fetch": "3.0.6",
    "dotenv-flow": "3.2.0",
    "dotenv-flow-cli": "1.0.0",
    "es6-promise": "4.2.8",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-config-react": "^1.1.7",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-lodash": "^7.3.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.9.0",
    "jest": "26.6.3",
    "jest-fetch-mock": "3.0.3",
    "serve": "11.3.2",
    "start-server-and-test": "1.11.0",
    "ts-node": "9.0.0",
    "tsconfig-paths": "3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "4.1.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
