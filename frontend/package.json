{
  "name": "@pachyderm/dash-frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "build": "NODE_ENV=production dotenv-flow -p ../ -- tsc && NODE_ENV=production dotenv-flow -p ../ -- vite build",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "npx eslint '{src,cypress}/**/*.{ts,tsx}'",
    "lint:css": "npx stylelint 'src/**/*.module.css'",
    "serve": "vite preview",
    "start:mock": "NODE_ENV=test dotenv-flow -p ../ -- vite",
    "start": "NODE_ENV=development dotenv-flow -p ../ -- vite",
    "test": "NODE_ENV=test GRAPHQL_PORT=0 LOG_LEVEL=none dotenv-flow -p ../ -- jest --max-workers=6",
    "tslint": "npx tslint 'src/**/*.{ts,tsx}'",
    "generate-hooks": "graphql-codegen"
  },
