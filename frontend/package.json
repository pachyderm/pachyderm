{
  "name": "@pachyderm/dash-frontend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true npm run build",
    "build": "NODE_ENV=production dotenv-flow -p ../ -- tsc && NODE_ENV=production dotenv-flow -p ../ -- vite build",
    "lint": "npm run lint:js",
    "lint:js": "npx eslint --fix '{src,components}/**/*.{ts,tsx}'",
    "lint:js:no-fix": "npx eslint '{src,components}/**/*.{ts,tsx}'",
    "lint:css": "npx stylelint '{src,components}/**/*.module.css'",
    "serve": "vite preview",
    "start": "NODE_ENV=development dotenv-flow -p ../ -- vite",
    "start:e2e-test": "NODE_ENV=test dotenv-flow -p ../ -- vite",
    "start:env-test": "NODE_ENV=mock dotenv-flow -p ../ -- vite",
    "test": "npm run test:components && npm run test:frontend",
    "test:frontend": "NODE_ENV=test GRAPHQL_PORT=0 LOG_LEVEL=none npx dotenv-flow -p ../ -- npx jest --config ./src/jest.config.js --max-workers=6",
    "test:components": "NODE_ENV=test GRAPHQL_PORT=0 LOG_LEVEL=none dotenv-flow -p ../ -- jest --projects components --max-workers=6",
    "generate-hooks": "graphql-codegen",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:publish": "npm run storybook:build && netlify deploy --dir=storybook-static --prod"
  },
