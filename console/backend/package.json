{
  "// To install dependencies with Bazel-managed pnpm, run": "bazel run @pnpm//:pnpm -- --dir $PWD install",
  "name": "@pachyderm/dash-backend",
  "version": "0.0.1",
  "private": true,
  "proxy": "http://localhost:3000",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json && cp -r ./src/proto/proto ./dist/proto/proto",
    "lint": "eslint --fix 'src/**/*.{ts,tsx}'",
    "lint:no-fix": "eslint 'src/**/*.{ts,tsx}'",
    "start": "NODE_PATH=dist/ NODE_ENV=production DOTENV_FLOW_PATH=../ node -r module-alias/register -r dotenv-flow/config dist/index.js",
    "start:dev": "LOG_LEVEL=debug NODE_ENV=development DOTENV_FLOW_PATH=../ nodemon --watch ./src --ext ts -- --files -r dotenv-flow/config -r tsconfig-paths/register src/index.ts",
    "start:test": "NODE_ENV=mock DOTENV_FLOW_PATH=../ nodemon --watch ./src --ext ts -- --files -r dotenv-flow/config -r tsconfig-paths/register src/index.ts",
    "test": "NODE_ENV=test GRAPHQL_PORT=0 LOG_LEVEL=none npx dotenv-flow -p ../ -- npx jest --config jest.config.js --max-workers=6 --selectProjects test"
  },
  "dependencies": {
    "@grpc/grpc-js": "1.10.9",
    "@sentry/node": "7.64.0",
    "@sentry/tracing": "7.114.0",
    "@types/express": "^4.17.21",
    "@types/google-protobuf": "^3.15.6",
    "@types/node": "^20.11.20",
    "apollo-server-errors": "^3.3.1",
    "bunyan": "1.8.15",
    "busboy": "1.5.0",
    "compression": "^1.7.4",
    "cookie-parser": "1.4.6",
    "dotenv-flow": "3.2.0",
    "dotenv-flow-cli": "1.1.1",
    "ejs": "3.1.10",
    "express": "4.19.2",
    "google-protobuf": "^3.21.2",
    "jsonwebtoken": "9.0.2",
    "lodash": "4.17.21",
    "module-alias": "2.2.2",
    "openid-client": "4.4.0",
    "p-queue": "6.6.2",
    "retry": "^0.13.1",
    "uuid": "9.0.1",
    "zstd-codec": "^0.1.4"
  },
  "devDependencies": {
    "@swc/core": "^1.5.27",
    "@swc/jest": "^0.2.36",
    "@types/bunyan": "1.8.8",
    "@types/busboy": "1.5.0",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "1.4.7",
    "@types/cors": "^2.8.17",
    "@types/ejs": "3.1.5",
    "@types/jest": "^29.5.4",
    "@types/jsonwebtoken": "9.0.6",
    "@types/lodash": "4.17.0",
    "@types/mime-types": "2.1.1",
    "@types/retry": "^0.12.5",
    "@types/uuid": "9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "cors": "^2.8.5",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-testing-library": "^5.9.1",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jest-junit": "^16.0.0",
    "node-mocks-http": "^1.13.0",
    "nodemon": "3.1.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.1.1",
    "typescript": "5.6.2"
  },
  "_moduleAliases": {
    "@dash-backend": "./dist"
  }
}
