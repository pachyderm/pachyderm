{
  "metadata": {
    "name": "pachd",
    "creationTimestamp": null,
    "labels": {
      "app": "pachd",
      "suite": "pachyderm"
    }
  },
  "spec": {
    "replicas": 2,
    "selector": {
      "app": "pachd"
    },
    "template": {
      "metadata": {
        "name": "pachd",
        "creationTimestamp": null,
        "labels": {
          "app": "pachd",
          "suite": "pachyderm"
        }
      },
      "spec": {
        "volumes": [
          {
            "name": "pach-disk",
            "hostPath": {
              "path": "/tmp/pach"
            }
          }
        ],
        "containers": [
          {
            "name": "pachd",
            "image": "pachyderm/pachd",
            "ports": [
              {
                "name": "api-grpc-port",
                "containerPort": 650,
                "protocol": "TCP"
              },
              {
                "name": "trace-port",
                "containerPort": 1050
              }
            ],
            "env": [
              {
                "name": "PACH_ROOT",
                "value": "/pach"
              },
              {
                "name": "NUM_SHARDS",
                "value": "32"
              },
              {
                "name": "STORAGE_BACKEND"
              },
              {
                "name": "PACHD_POD_NAMESPACE",
                "valueFrom": {
                  "fieldRef": {
                    "apiVersion": "v1",
                    "fieldPath": "metadata.namespace"
                  }
                }
              }
            ],
            "resources": {},
            "volumeMounts": [
              {
                "name": "pach-disk",
                "mountPath": "/pach"
              }
            ],
            "readinessProbe": {
              "initialDelaySeconds": 15,
              "timeoutSeconds": 1,
              "exec": {
                "command": [
                  "./pachd",
                  "--readiness-check"
                ]
              }
            },
            "imagePullPolicy": "IfNotPresent",
            "securityContext": {
              "privileged": true
            }
          }
        ],
        "serviceAccountName": "pachyderm"
      }
    }
  },
  "status": {
    "replicas": 0
  },
  "kind": "ReplicationController",
  "apiVersion": "v1"
}
