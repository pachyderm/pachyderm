{
  "metadata": {
    "name": "etcd",
    "creationTimestamp": null,
    "labels": {
      "app": "etcd",
      "suite": "pachyderm"
    }
  },
  "spec": {
    "replicas": 1,
    "selector": {
      "app": "etcd"
    },
    "template": {
      "metadata": {
        "name": "etcd",
        "creationTimestamp": null,
        "labels": {
          "app": "etcd",
          "suite": "pachyderm"
        }
      },
      "spec": {
        "volumes": [
          {
            "name": "etcd-storage"
          }
        ],
        "containers": [
          {
            "name": "etcd",
            "image": "gcr.io/google_containers/etcd:2.0.12",
            "command": [
              "/usr/local/bin/etcd",
              "--bind-addr=0.0.0.0:2379",
              "--data-dir=/var/etcd/data"
            ],
            "ports": [
              {
                "name": "client-port",
                "containerPort": 2379
              },
              {
                "name": "peer-port",
                "containerPort": 2380
              }
            ],
            "resources": {},
            "volumeMounts": [
              {
                "name": "etcd-storage",
                "mountPath": "/var/data/etcd"
              }
            ],
            "imagePullPolicy": "IfNotPresent"
          }
        ]
      }
    }
  },
  "status": {
    "replicas": 0
  },
  "kind": "ReplicationController",
  "apiVersion": "v1"
}
