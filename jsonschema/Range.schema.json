{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/Range",
    "definitions": {
        "Range": {
            "properties": {
                "offset": {
                    "type": "integer"
                },
                "last_path": {
                    "type": "string"
                },
                "chunk_ref": {
                    "$ref": "#/definitions/chunk.DataRef",
                    "additionalProperties": false
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Range"
        },
        "chunk.DataRef": {
            "properties": {
                "ref": {
                    "$ref": "#/definitions/chunk.Ref",
                    "additionalProperties": false,
                    "description": "The chunk the referenced data is located in."
                },
                "hash": {
                    "type": "string",
                    "description": "The hash of the data being referenced.",
                    "format": "binary",
                    "binaryEncoding": "base64"
                },
                "offset_bytes": {
                    "type": "integer",
                    "description": "The offset and size used for accessing the data within the chunk."
                },
                "size_bytes": {
                    "type": "integer"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Data Ref",
            "description": "DataRef is a reference to data within a chunk."
        },
        "chunk.Ref": {
            "properties": {
                "id": {
                    "type": "string",
                    "format": "binary",
                    "binaryEncoding": "base64"
                },
                "size_bytes": {
                    "type": "integer"
                },
                "edge": {
                    "type": "boolean"
                },
                "dek": {
                    "type": "string",
                    "format": "binary",
                    "binaryEncoding": "base64"
                },
                "encryption_algo": {
                    "enum": [
                        "ENCRYPTION_ALGO_UNKNOWN",
                        "CHACHA20"
                    ],
                    "type": "string",
                    "title": "Encryption Algo"
                },
                "compression_algo": {
                    "enum": [
                        "NONE",
                        "GZIP_BEST_SPEED"
                    ],
                    "type": "string",
                    "title": "Compression Algo"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Ref"
        }
    }
}