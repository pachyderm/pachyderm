                        {
                            "type": "null"
                        },
                        {}
                    ]
                },
                "reprocess_spec": {
                    "oneOf": [
                        {
                            "type": "null"
                        },
                        {
                            "type": "string"
                        }
                    ]
                },
                "autoscaling": {
                    "oneOf": [
                        {
                            "type": "null"
