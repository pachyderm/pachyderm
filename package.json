{
  "name": "@pachyderm/haberdashery",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "check-version": "node scripts/checkVersion.js",
    "cypress:ci": "NODE_ENV=test dotenv-flow -- cypress run --browser chrome --config video=false --record --parallel --group e2e",
    "cypress:open": "NODE_ENV=test dotenv-flow -- cypress open",
    "cypress:headless": "NODE_ENV=test dotenv-flow -- cypress run --browser chrome --config video=false",
    "e2e": "start-server-and-test start:e2e http-get://localhost:4000 cypress:ci",
    "generate-types": "graphql-codegen",
    "setup": "node setup.js",
    "start:e2e": "LOG_LEVEL=debug npm run start:dev --prefix ./backend & npm run start --prefix ./frontend",
    "start:mock": "LOG_LEVEL=debug npm run mock-and-start --prefix ./backend & npm run start:mock  --prefix ./frontend",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@cypress/code-coverage": "3.9.12",
    "@graphql-codegen/add": "2.0.2",
    "@graphql-codegen/cli": "1.19.2",
    "@graphql-codegen/import-types-preset": "1.18.1",
    "@graphql-codegen/typescript": "1.20.0",
    "@graphql-codegen/typescript-operations": "1.17.15",
    "@graphql-codegen/typescript-react-apollo": "2.2.3",
    "@graphql-codegen/typescript-resolvers": "1.18.1",
    "@testing-library/cypress": "8.0.2",
    "cypress": "9.2.0",
    "cypress-wait-until": "1.7.1",
    "dotenv-flow": "3.2.0",
    "dotenv-flow-cli": "1.0.0",
    "envfile": "6.14.0",
    "graphql": "15.5.0",
    "react-transition-group": "4.4.2",
    "start-server-and-test": "1.14.0",
    "typescript": "4.1.2"
  }
}
